
<section class="section section-primary section-no-border border-0 mb-0 appear-animation" data-appear-animation="fadeIn" data-plugin-options="{'accY': -150}">
	<div class="container">
		<div class="row counters counters-sm pb-4 pt-3">
			<div class="col-sm-6 col-lg-3 mb-5 mb-lg-0">
				<div class="counter">
					<i class="icons icon-user text-color-light"></i>
					<strong class="text-color-light font-weight-extra-bold" data-to="@ViewBag.Contect" data-append="+">0</strong>
					<label class="text-4 mt-1 text-color-light">Number of members </label>
				</div>
			</div>
			<div class="col-sm-6 col-lg-3 mb-5 mb-lg-0">
				<div class="counter">
					<i class="fas fa-city text-color-light"></i>
					<strong class="text-color-light font-weight-extra-bold" data-to="@ViewBag.Counter">0</strong>
					<label class="text-4 mt-1 text-color-light"> Number of projects</label>
				</div>
			</div>
			<div class="col-sm-6 col-lg-3 mb-5 mb-sm-0">
				<div class="counter">
					<i class="fas fa-home text-color-light"></i>
					<strong class="text-color-light font-weight-extra-bold" data-to="@ViewBag.sum">0</strong>
					<label class="text-4 mt-1 text-color-light">Number of units </label>
				</div>
			</div>
			<div class="col-sm-6 col-lg-3">
				<div class="counter">
					<i class="fas fa-eye text-color-light"></i>
					<strong class="text-color-light font-weight-extra-bold" data-to="@ViewBag.Visit">0</strong>
					<label class="text-4 mt-1 text-color-light">The number of site visits </label>
				</div>
			</div>
		</div>
	</div>
</section>